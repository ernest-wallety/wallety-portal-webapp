{
  "name": "wallety-portal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run start:development --no-hmr",
    "start:development": "NODE_ENV=development ng serve --no-hmr --port 3000",
    "start:local": "NODE_ENV=development ng serve --no-hmr --port 3000",
    "start:production": "NODE_ENV=production ng serve --no-hmr",
    "build": "ng build:development",
    "build:local": "NODE_ENV=development ng build",
    "build:development": "NODE_ENV=development ng build",
    "build:production": "NODE_ENV=production ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "install:clean": "rm -rf node_modules/ package-lock.json .angular/ && npm install",
    "serve:ssr:wallety-portal": "nodemon --watch dist/wallety-portal/server --exec node dist/wallety-portal/server/server.mjs",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "prepare": "husky install",
    "pretty-quick": "pretty-quick --staged"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/cdk": "^19.0.4",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/elements": "^19.0.5",
    "@angular/forms": "^19.0.0",
    "@angular/localize": "^19.0.5",
    "@angular/material": "^19.0.4",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/platform-server": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@angular/ssr": "^19.0.6",
    "@auth0/angular-jwt": "^5.2.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@ng-bootstrap/ng-bootstrap": "^18.0.0",
    "@ng-select/ng-select": "^14.1.0",
    "@popperjs/core": "^2.11.8",
    "bootstrap": "^5.3.3",
    "boxicons": "^2.1.4",
    "chart.js": "^4.4.7",
    "clipboard": "^2.0.11",
    "express": "^4.18.2",
    "ngx-clipboard": "^16.0.0",
    "ngx-color-picker": "^17.0.0",
    "ngx-daterangepicker-material": "^6.0.4",
    "ngx-toastr": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@angular/language-service": "^19.0.5",
    "@eslint/js": "^9.21.0",
    "@ngx-env/builder": "^19.0.4",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "angular-eslint": "19.0.2",
    "eslint": "^9.21.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.3.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.4.2",
    "pretty-quick": "^4.0.0",
    "typescript": "~5.6.2",
    "typescript-eslint": "8.18.0"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "ng lint --fix",
      "git add"
    ],
    "src/**/*.html": [
      "ng lint --fix",
      "git add"
    ],
    "src/**/*.scss": [
      "ng lint --fix",
      "git add"
    ]
  }
}
